class Solution {
    static int smallestSumSubarray(int a[], int size) {
           
           int currmin = a[0];
           int minSoFar = a[0];
           
           for(int i = 1; i < a.length; i++){
               currmin = Math.min(a[i],currmin + a[i]);
               minSoFar = Math.min(minSoFar,currmin);
           }
           return minSoFar;
        
    }
}
